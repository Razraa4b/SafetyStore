{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "outDir": "./wwwroot/js",
    "rootDir": "./static/ts",
    "sourceMap": true
  },
  "include": [ "static/ts/**/*" ],
  "exclude": [ "node_modules" ]
}
